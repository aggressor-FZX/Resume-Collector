# Example environment variables (DO NOT include secrets in this file)
# Copy this to `.env` and fill in the real values.

# GitHub API
GITHUB_API_KEY=

# Stack Exchange
STACK_EXCHANGE_API_KEY=

# Semantic Scholar
SEMANTIC_SCHOLAR_API_KEY=

# Perplexity / Research Provider
# Two possible names used by different tools
PERPLEXITY_API_KEY=
PERPLEXITY_AI_API_KEY=

# Anthropic (if used)
ANTHROPIC_API_KEY=

# Other provider keys
DEEPSEEK_API_KEY=
OPENROUTER_API_KEY=

# Hugging Face (for dataset upload)
HUGGING_FACE_API_KEY=

# Project settings
DEFAULT_SUBTASKS=5
DEFAULT_PRIORITY=medium

# Rate limits (optional)
GITHUB_RATE_LIMIT=5000
STACK_EXCHANGE_RATE_LIMIT=1000
SEMANTIC_SCHOLAR_RATE_LIMIT=1000
OPENALEX_RATE_LIMIT=10
